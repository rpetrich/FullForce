entry = {
  bundle = AppList;
  cell = PSLinkCell;
  icon = "/Library/PreferenceLoader/Preferences/FullForce.png";
  isController = 1;
  label = FullForce;
  ALSettingsPath = "/var/mobile/Library/Preferences/com.booleanmagic.fullforce.plist";
  ALSettingsKeyPrefix = "FFEnabled-";
  ALChangeNotification = "com.booleanmagic.fullforce.settingschanged";
  ALAllowsSelection = 1;
  ALSectionDescriptors = (
  	{
  	  items = (
  	    {
  	      text = "RetinaPad";
  	      image = "/Library/PreferenceLoader/Preferences/FullForce-RetinaPad.png";
  	      action = "launchURL";
  	      url = "cydia://package/com.rpetrich.retinapad";
  	    }
  	  );
  	  "footer-title" = "Get RetinaPad for a better 2× mode. It supports many more apps and games than FullForce by simulating the iPhone's Retina Display";
  	},
  	{
  	  title = "Classic Applications";
  	  predicate = "(isActuallyClassic = TRUE) AND NOT (tags contains 'no-fullforce')";
  	  "cell-class-name" = "ALSwitchCell";
  	  "icon-size" = 29;
  	  "suppress-hidden-apps" = 1;
      "footer-title" = "© 2010-2014 Ryan Petrich";
  	}
  );
};